
root  ::= list


list ::= 
    "[" ws (
        object
        ("," ws object)*
        )? "]"


object ::=  "{"("\"firstname\"" ":" string ",""\"lastname\"" ":" string ",""\"phone\"" ":" number ",")"}" 

array  ::=
  "[" ws (
            string
    ("," ws string)*
  )? "]"

string  ::=
  "\"" (
    [^"\] |
    "\\" (["\/bfnrt] | "u" [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F])
  )* "\"" ws

ws ::= ([ \t\n] ws)?
